generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Playlist {
  id         Int      @id @default(autoincrement())
  url        String   @unique
  platform   String
  title      String?
  curator    String?
  coverImage String?
  gradient   String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
